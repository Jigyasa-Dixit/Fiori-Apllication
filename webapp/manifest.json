{
    "_version": "1.43.0",
    "sap.app":{
        "id": "com.soyuz.team.it",
        "type": "application",
        "ach":"",
        "applicationVersion":{
            "version":"1.0.0"
        },
        "title":"{{appTitle}}",
        "description":"{{appDescription}}",
        "i18n":"i18n/i18n.properties"
    },
    "sap.ui":{
        "deviceTypes":{
            "desktop":true,
            "phone":true,
            "tablet":true
        },
        "supportedThemes":["sap_fiori_3","sap_fiori_3_dark"],
        "fullWidth":true,
        "technology":"UI5"
    },
    "sap.ui5":{
        "dependencies":{
            "minUI5Version":"1.100",
            "libs":{
                "sap.ui.core":{ "minUI5Version":"1.100"},
                "sap.m":{},
                "sap.ui.layout":{}
            },
            "contentDensities":{
                "compact":true,
                "cozy":true
            }            
        },
        "models":{
            "":{
                "type":"sap.ui.model.json.JSONModel",
                "uri":"mockdata/fruits.json",
                "preload":false,
                "settings":{}
            },
            "got":{
                "type":"sap.ui.model.json.JSONModel",
                "uri":"mockdata/fruits.json",
                "preload":false,
                "settings":{}
            },
            
            "i18n":{
                "type":"sap.ui.model.resource.ResourceModel",
                "preload":true,
                "settings":{                        
                    "bundleUrl":"i18n/i18n.properties"
                }
            }
        },
        "rootView":{
            "id":"idRootView",
            "viewName":"com.soyuz.team.it.view.App",
            "type":"XML"
           },
           "routing":{
            "config":{
                "routerClass":"sap.m.routing.Router",
                "viewPath":"com.soyuz.team.it.view",
                "viewType":"XML",
                "controlId":"idAppCon"
            },
            "routes":[
                {
                    "pattern":"",
                    "name":"spiderman",
                    "target":["joe","biden"]
                },
                {
                    "pattern":"fruits/{fruitId}",
                    "name":"superman",
                    "target":["joe","modi"]
                },{
                    "pattern":"suppliers/{suppId}",
                    "name":"ironman",
                    "target":["joe","putin"]
                }
            ],
            "targets":{
                "joe":{
                    "viewId":"idView1",
                    "viewName":"View1",
                    "controlAggregation":"masterPages"
                },
                "biden":{
                    "viewId":"idEmpty",
                    "viewName":"Empty",
                    "controlAggregation":"detailPages"
                },
                "modi":{
                    "viewId":"idView2",
                    "viewName":"View2",
                    "controlAggregation":"detailPages"
                },
                "putin":{
                    "viewId":"idView3",
                    "viewName":"View3",
                    "controlAggregation":"detailPages"
                }
            }
           }
        }
    }

